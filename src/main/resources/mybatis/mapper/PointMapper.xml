<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN "
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zftx.mcdaily.mapper.PointMapper">
    <resultMap id="point" type="com.zftx.mcdaily.bean.Point">
        <id column="id" property="id"/>
        <result column="type_id" property="typeId"/>
        <result column="surface_id" property="surfaceId"/>
        <result column="line_id" property="lineId"/>
        <result column="point_id" property="pointId"/>
        <result column="point_name" property="pointName"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>
    <sql id="columnAll">id,type_id,surface_id,line_id,point_id,point_name,create_user,create_time,update_user,update_time</sql>
    <sql id="columnBase">id,type_id,surface_id,line_id,point_id,point_name</sql>
</mapper>